
@{
    ViewBag.Title = "SearchForm";
}

<form class="searchForm" id="searchForm">
    <div class="form-group">
        <select class="form-control selectForm" id="ward">
            <option value="" selected>--- Phường/Xã ---</option>
            <option value="tanphu">Tân Phú</option>
        </select>
    </div>
    <div class="form-group">
        <select class="form-control selectForm" id="province">
            <option value="" selected>--- Quận/Huyện ---</option>
            <option value="tanphu">Tân Phú</option>
        </select>
    </div>
    <div class="form-group">
        <input type="text" class="form-control" id="soThuTuThua" />
    </div>
    <div class="form-group">
        <input type="text" class="form-control" id="soHieuToBanDo" />
    </div>

    <div>
        <button id="btnSearch" class="btn btn-primary"><i class="fa-solid fa-magnifying-glass"></i> Search</button>
    </div>
</form>
